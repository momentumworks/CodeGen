// MARK: - Immutable

{% for object in extensions.Immutable %}
{% if object.fields %}
extension {{ object.name }} {
	{% for field in object.fields %}
	{{ field.accessibility }} func set({{ field.name }} {{ field.name }}: {{ field.type }} -> {{ object.name }} {
		return {{ object.constructorCall }}
	}
	{% endfor %}
}
{% endif %}
{% endfor %}
